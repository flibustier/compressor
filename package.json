{
  "name": "c8r",
  "version": "1.0.3",
  "private": false,
  "description": "encode/decode array of numbers into optimized Base 64 URL compatible",
  "author": "Flibustier",
  "keywords": [
    "javascript",
    "base 64",
    "url shortener",
    "compression",
    "URL",
    "dynamic bit length"
  ],
  "homepage": "https://github.com/flibustier/compressor#readme",
  "repository": "github:flibustier/compressor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier src/** src/**/*.{js,ts} --write",
    "lint": "eslint src/**",
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.29.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.4",
    "prettier": "^2.3.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "jest"
    ],
    "parser": "babel-eslint",
    "rules": {
      "sort-imports": "warn",
      "sort-keys": "warn"
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.m?(j|t)s$": "babel-jest"
    },
    "collectCoverage": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT"
}
